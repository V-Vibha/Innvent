<mvc:View controllerName="com.sap.bpm.pv.innvent.ui.controller.MyView" 
    xmlns="sap.m"
    xmlns:s="sap.m.Sticky"
    xmlns:core="sap.ui.core"
    xmlns:viz="sap.viz.ui5.controls" 
    xmlns:layout="sap.ui.layout" displayBlock="true"
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data" height="100%">

<Page id="page" >
    <layout:FixFlex id='chartFixFlex'>
       <layout:flexContent>
       <HBox height="100%" width="100%">
            <VBox height="100%"> 
                <!-- <s:InfoToolbar > -->
                    <OverflowToolbar id="_IDGenOverflowToolbar1" class="sapUiMediumMarginBegin sapUiSmallMarginEnd" width="300px" height="50px">
                        <Title level="H3" text="Slice and Dice by" />
                    </OverflowToolbar>
                <!-- </s:InfoToolbar> width="1010px"-->
                <List id ="attributeList" items="{/}" class="sapUiMediumMarginBegin sapUiSmallMarginEnd" height="100%" width="300px">
                    <items>
                        <CustomListItem>
                            <RadioButton text="{label}" select="handleSelectChange">
                            <customData>
                                <core:CustomData key="keyy" value="{key}"/>
                            </customData></RadioButton>
                        </CustomListItem>
                    </items>
                </List>
                <!-- <RadioButtonGroup items="{/}" select="handleSelectChange">
                    <RadioButton text="{label}" />
                </RadioButtonGroup> -->
            </VBox>
            <viz:VizFrame vizType="treemap" id="myTreeMap" class="sapUiMediumMarginEnd" width="1010px" height="100%">
                <viz:feeds id="vizfeed">
                    <viz.feeds:FeedItem uid="color" type="Measure"
                        values="Number Of Instances" />
                    <viz.feeds:FeedItem uid="weight" type="Measure"
                        values="Number Of Instances" />                     
                    <viz.feeds:FeedItem uid="title" type="Dimension"
                        values="Status" />    
                    <viz.feeds:FeedItem id="IDGenFeedItem4" uid="title" type="Dimension"
                    values="Selected Attribute" />
                </viz:feeds>
            </viz:VizFrame>
          </HBox>  
        </layout:flexContent>
    </layout:FixFlex>
</Page>
</mvc:View>